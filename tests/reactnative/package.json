    "@types/react": "18.2.38",
    "@web5/api": "0.8.4-rc.0",
    "@web5/dids": "0.2.3",
    "@web5/identity-agent": "0.2.5",
    "expo": "49.0.20",
    "expo-splash-screen": "0.22.0",
    "expo-status-bar": "1.7.1",
    "memory-level": "1.0.0",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-bignumber": "0.2.2",
    "react-native-blob-jsi-helper": "^0.3.1",
